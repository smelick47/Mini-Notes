<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <ClientWebPart 
	  Name="Mini Notes"
	  Title="Mini Notes"
	  Description="You can view your notes by adding this app part."
	  DefaultWidth="200"
	  DefaultHeight="90"
	  >

    <!-- Content element identifies the location of the page that will render inside the client web part
         Properties are referenced on the query string using the pattern _propertyName_
         Example: Src="~appWebUrl/Pages/ClientWebPart1.aspx?Property1=_property1_" -->
    <Content Type="html" Src="~appWebUrl/Pages/AppPartPages/Notes.aspx?{StandardTokens}&amp;DisplayType=iframe" />

    <!-- Define properties in the Properties element
           Remember to put Property Name on the Src attribute of the Content element above
    <Properties>
      <Property Name="property1" Type="string" WebBrowsable="true" WebDisplayName="First Property" WebDescription="Description 1" WebCategory="Custom Properties" DefaultValue="String Property" RequiresDesignerPermission="true" />
    </Properties>   -->

  </ClientWebPart>
</Elements>
